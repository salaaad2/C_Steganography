#include "../inc/stega.h"

void	read_dc(char *f_search)
{
	int i;
	FILE  *fp, *f2; 
	size_t len, len2;
	char *line_s;
	char *line_d;

	i = 0;
	len = 0;
	len2 = 0;
	fp = fopen(f_search, "r");
	f2 = fopen("img/barbara.ascii.pgm", "r");
	getline(&line_s, &len, fp);
	getline(&line_d, &len2, f2);
	printf("target line :: %s\nsource line :: %s\n", line_d, line_s);
	i++;
}

